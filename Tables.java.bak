import java.util.*;
class  Tables
{
	static Scanner sc=new Scanner (System.in);
	public static void main(String[] args) 
	{
		int tab;
		
		System.out.print("\t\t\tEnter the Tables Number :");
		tab=sc.nextInt();
		
		for (int i=1;i<=10 ;i++ )
		{
			System.out.println("\t\t\t"tab+"x"+1+"="+tab*i);
		}
	}
}
