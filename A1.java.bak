import java.util.*;
class A1
{
	static Scanner sc=new Scanner (System.in);
	public static void main(String[]args)
	{
		int temp;
		System.out.print("Enter the number : ");
		int num=sc.nextInt();
		
		int rev=0;
		
		while (num!=0)
		{
			rev=rev*10+num%10;
			num/=10;
		}
		if (num==rev)
		{
			System.out.println("Palindrome");
		}
		else 
			System.out.println(" not a Palindrome");
		
	}
}
